uario:', error);\n            }\n            if (!this.$store.state.email){\n                this.$store.commit('login',this.usuario.nombreUsuario); \n            }\n            \n        }\n    }\n}\n</script>\n\n<style>\n\n.nav-color {\n    background-color: #073769 !important;\n}\n.title-nav {\n    color: white !important;\n    font-family: \"Roboto\", \"sans-serif\";\n    font-weight: 400;\n}\n.title-nav-bar-text {\n    font-family: \"Roboto\", \"sans-serif\";\n    font-weight: 300;\n}\n\n</style>"],"mappings":"AAmDA,OAAAA,KAAA;AAIA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;QACAC,aAAA;QACAC,UAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,aAAA;MACAC,OAAA,CAAAC,GAAA,MAAAN,OAAA,CAAAC,aAAA,eAAAD,OAAA,CAAAE,UAAA;IACA;IACAK,OAAA;MACA;QACA,MAAAC,QAAA,GAAAX,KAAA,CAAAY,GAAA;QACAJ,OAAA,CAAAC,GAAA;QACA,WAAAN,OAAA,IAAAQ,QAAA,CAAAT,IAAA;UACAM,OAAA,CAAAC,GAAA,CAAAN,OAAA;QACA;MACA,SAAAU,KAAA;QACAL,OAAA,CAAAK,KAAA,gCAAAA,KAAA;MACA;MACA,UAAAC,MAAA,CAAAC,KAAA,CAAAC,KAAA;QACA,KAAAF,MAAA,CAAAG,MAAA,eAAAd,OAAA,CAAAC,aAAA;MACA;IAEA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}